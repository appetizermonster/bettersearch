{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "lib": [
      "es2015",
      "dom"
    ]
  }
}
